{
  "name": "@uglyteens/cross-iframe-resize",
  "private": true,
  "version": "0.0.1",
  "author": "keguigong<keguigong@gmail.com>",
  "packageManager": "pnpm@10.2.1",
  "main": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "default": "./src/*.ts"
    }
  },
  "publishConfig": {
    "main": "./dist/index.mjs",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "default": "./dist/index.mjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "ahooks": "^3.8.4",
    "birpc": "^2.0.19"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.0",
    "@eslint/js": "^9.12.0",
    "@rsbuild/core": "^1.1.8",
    "@rsbuild/plugin-less": "^1.1.0",
    "@rsbuild/plugin-react": "^1.0.7",
    "@rslib/core": "^0.4.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "eslint": "^9.12.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.11.0",
    "husky": "^8.0.0",
    "prettier": "^3.3.3",
    "tailwindcss": "^4.0.4",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.8.1"
  }
}
